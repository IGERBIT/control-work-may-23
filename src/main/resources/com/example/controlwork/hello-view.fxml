<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_LEFT" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controlwork.TestController">
    <TabPane fx:id="tabPane" prefHeight="400.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab text="1">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                </content>
            </Tab>
            <Tab text="2">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                </content>
            </Tab>
        </tabs>
    </TabPane>
   <AnchorPane prefWidth="200.0">
      <children>
         <Button fx:id="leftButton" mnemonicParsing="false" onAction="#onLeftButtonClick" text="Назад" AnchorPane.leftAnchor="0.0" />
         <Button fx:id="rightButton" mnemonicParsing="false" onAction="#onRightButtonClick" text="Далее" AnchorPane.rightAnchor="5.0" />
      </children>
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
   </AnchorPane>
</VBox>
